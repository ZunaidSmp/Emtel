<?xml version="1.0" encoding="UTF-8"?>
<CustomField xmlns="http://soap.sforce.com/2006/04/metadata">
    <fullName>Account_Active_Period__c</fullName>
    <description>This field will show the time period for which account is active</description>
    <externalId>false</externalId>
    <formula>IF(ISBLANK( Account_Activated_Date__c  ), 0, TODAY() -  Account_Activated_Date__c  )</formula>
    <formulaTreatBlanksAs>BlankAsZero</formulaTreatBlanksAs>
    <label>Account Active Period</label>
    <precision>18</precision>
    <required>false</required>
    <scale>0</scale>
    <trackHistory>false</trackHistory>
    <type>Number</type>
    <unique>false</unique>
</CustomField>
